{
   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Test",
         "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
         "args": ["--runInBand", "--config"],
         "sourceMaps": true,
         "outFiles": ["${workspaceRoot}/output/debug/**/*"]
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Jest",
         "port": 9229,
         "address": "localhost",
         "stopOnEntry": false,
         "sourceMaps": true,
         "runtimeArgs": [
            "--inspect-brk",
            "./node_modules/jest-cli/bin/jest.js",
            "--runInBand",
            "cache.test.ts",
            "--testNamePattern",
            "automatically loads"
         ],
         "cwd": "${workspaceRoot}"
      }
   ]
}
